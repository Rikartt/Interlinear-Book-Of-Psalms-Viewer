{"chapter": 101, "verse": 1, "english": "I will sing of mercy and judgment: unto thee, O LORD, will I sing.", "hebrew": [["ל/דוד", "Hl/H1732", "HR/Np"], ["מזמור", "H4210", "HNcmsa"], ["חסד", "H2617", "HNcmsa"], ["ו/משפט", "Hc/H4941", "HC/Ncmsa"], ["אשירה", "H7891", "HVqh1cs"], ["ל/ך", "Hl", "HR/Sp2ms"], ["יהוה", "H3068", "HNp"], ["אזמרה", "H2167", "HVph1cs"]], "header": "¶ A Psalm of David."}
{"chapter": 101, "verse": 2, "english": "I will behave myself wisely in a perfect way. O when wilt thou come unto me? I will walk within my house with a perfect heart.", "hebrew": [["אשכילה", "H7919", "HVhh1cs"], ["ב/דרך", "Hb/H1870", "HR/Ncbsa"], ["תמים", "H8549", "HAamsa"], ["מתי", "H4970", "HTi"], ["תבוא", "H935", "HVqi2ms"], ["אל/י", "H413", "HR/Sp1cs"], ["אתהלך", "H1980", "HVti1cs"], ["ב/תם", "Hb/H8537", "HR/Ncmsc"], ["לבב/י", "H3824", "HNcmsc/Sp1cs"], ["ב/קרב", "Hb/H7130", "HR/Ncmsc"], ["בית/י", "H1004", "HNcmsc/Sp1cs"]], "header": ""}
{"chapter": 101, "verse": 3, "english": "I will set no wicked thing before mine eyes: I hate the work of them that turn aside; <i>it</i> shall not cleave to me.", "hebrew": [["לא", "H3808", "HTn"], ["אשית", "H7896", "HVqi1cs"], ["ל/נגד", "Hl/H5048", "HR/R"], ["עינ/י", "H5869", "HNcbdc/Sp1cs"], ["דבר", "H1697", "HNcmsc"], ["בליעל", "H1100", "HNcmsa"], ["עשה", "H6213", "HVqc"], ["סטים", "H7750", "HNcmpa"], ["שנאתי", "H8130", "HVqp1cs"], ["לא", "H3808", "HTn"], ["ידבק", "H1692", "HVqi3ms"], ["ב/י", "Hb", "HR/Sp1cs"]], "header": ""}
{"chapter": 101, "verse": 4, "english": "A froward heart shall depart from me: I will not know a wicked <i>person.</i>", "hebrew": [["לבב", "H3824", "HNcmsa"], ["עקש", "H6141", "HAamsa"], ["יסור", "H5493", "HVqi3ms"], ["ממ/ני", "H4480", "HR/Sp1cs"], ["רע", "H7451", "HAamsa"], ["לא", "H3808", "HTn"], ["אדע", "H3045", "HVqi1cs"]], "header": ""}
{"chapter": 101, "verse": 5, "english": "Whoso privily slandereth his neighbour, him will I cut off: him that hath an high look and a proud heart will not I suffer.", "hebrew": [["מלושני", "H3960", "HVmrmsc"], ["מלשני", "H3960", "HVmrmsc"], ["ב/סתר", "Hb/H5643", "HRd/Ncmsa"], ["רע/הו", "H7453", "HNcmsc/Sp3ms"], ["אות/ו", "H853", "HTo/Sp3ms"], ["אצמית", "H6789", "HVhi1cs"], ["גבה", "H1362", "HAamsc"], ["עינים", "H5869", "HNcbda"], ["ו/רחב", "Hc/H7342", "HC/Aamsc"], ["לבב", "H3824", "HNcmsa"], ["את/ו", "H853", "HTo/Sp3ms"], ["לא", "H3808", "HTn"], ["אוכל", "H3201", "HVqi1cs"]], "header": ""}
{"chapter": 101, "verse": 6, "english": "Mine eyes <i>shall be</i> upon the faithful of the land, that they may dwell with me: he that walketh in a perfect way, he shall serve me.", "hebrew": [["עינ/י", "H5869", "HNcbdc/Sp1cs"], ["ב/נאמני", "Hb/H539", "HR/VNrmpc"], ["ארץ", "H776", "HNcbsa"], ["ל/שבת", "Hl/H3427", "HR/Vqc"], ["עמד/י", "H5978", "HR/Sp1cs"], ["הלך", "H1980", "HVqrmsa"], ["ב/דרך", "Hb/H1870", "HR/Ncbsa"], ["תמים", "H8549", "HAamsa"], ["הוא", "H1931", "HPp3ms"], ["ישרת/ני", "H8334", "HVpi3ms/Sp1cs"]], "header": ""}
{"chapter": 101, "verse": 7, "english": "He that worketh deceit shall not dwell within my house: he that telleth lies shall not tarry in my sight.", "hebrew": [["לא", "H3808", "HTn"], ["ישב", "H3427", "HVqi3ms"], ["ב/קרב", "Hb/H7130", "HR/Ncmsc"], ["בית/י", "H1004", "HNcmsc/Sp1cs"], ["עשה", "H6213", "HVqrmsc"], ["רמיה", "H7423", "HNcfsa"], ["דבר", "H1696", "HVqrmsa"], ["שקרים", "H8267", "HNcmpa"], ["לא", "H3808", "HTn"], ["יכון", "H3559", "HVNi3ms"], ["ל/נגד", "Hl/H5048", "HR/R"], ["עינ/י", "H5869", "HNcbdc/Sp1cs"]], "header": ""}
{"chapter": 101, "verse": 8, "english": "I will early destroy all the wicked of the land; that I may cut off all wicked doers from the city of the LORD.", "hebrew": [["ל/בקרים", "Hl/H1242", "HRd/Ncmpa"], ["אצמית", "H6789", "HVhi1cs"], ["כל", "H3605", "HNcmsc"], ["רשעי", "H7563", "HAampc"], ["ארץ", "H776", "HNcbsa"], ["ל/הכרית", "Hl/H3772", "HR/Vhc"], ["מ/עיר", "Hm/H5892", "HR/Ncfsc"], ["יהוה", "H3068", "HNp"], ["כל", "H3605", "HNcmsc"], ["פעלי", "H6466", "HVqrmpc"], ["און", "H205", "HNcmsa"]], "header": ""}
