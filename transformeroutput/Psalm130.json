{"chapter": 130, "verse": 1, "english": "Out of the depths have I cried unto thee, O LORD.", "hebrew": [["שיר", "H7892", "HNcbsc"], ["ה/מעלות", "Hd/H4609", "HTd/Ncfpa"], ["מ/מעמקים", "Hm/H4615", "HR/Ncmpa"], ["קראתי/ך", "H7121", "HVqp1cs/Sp2ms"], ["יהוה", "H3068", "HNp"]], "header": "¶ A Song of degrees."}
{"chapter": 130, "verse": 2, "english": "Lord, hear my voice: let thine ears be attentive to the voice of my supplications.", "hebrew": [["אדנ/י", "H136", "HNcmpc/Sp1cs"], ["שמע/ה", "H8085", "HVqv2ms/Sh"], ["ב/קול/י", "Hb/H6963", "HR/Ncmsc/Sp1cs"], ["תהיינה", "H1961", "HVqi3fp"], ["אזני/ך", "H241", "HNcfdc/Sp2ms"], ["קשבות", "H7183", "HAafpa"], ["ל/קול", "Hl/H6963", "HR/Ncmsc"], ["תחנונ/י", "H8469", "HNcmpc/Sp1cs"]], "header": ""}
{"chapter": 130, "verse": 3, "english": "If thou, LORD, shouldest mark iniquities, O Lord, who shall stand?", "hebrew": [["אם", "H518", "HC"], ["עונות", "H5771", "HNcbpa"], ["תשמר", "H8104", "HVqi2ms"], ["יה", "H3050", "HNp"], ["אדנ/י", "H136", "HNcmpc/Sp1cs"], ["מי", "H4310", "HTi"], ["יעמד", "H5975", "HVqi3ms"]], "header": ""}
{"chapter": 130, "verse": 4, "english": "But <i>there is</i> forgiveness with thee, that thou mayest be feared.", "hebrew": [["כי", "H3588", "HC"], ["עמ/ך", "H5973", "HR/Sp2ms"], ["ה/סליחה", "Hd/H5547", "HTd/Ncfsa"], ["למען", "H4616", "HR"], ["תורא", "H3372", "HVNi2ms"]], "header": ""}
{"chapter": 130, "verse": 5, "english": "I wait for the LORD, my soul doth wait, and in his word do I hope.", "hebrew": [["קויתי", "H6960", "HVpp1cs"], ["יהוה", "H3068", "HNp"], ["קותה", "H6960", "HVpp3fs"], ["נפש/י", "H5315", "HNcbsc/Sp1cs"], ["ו/ל/דבר/ו", "Hc/Hl/H1697", "HC/R/Ncmsc/Sp3ms"], ["הוחלתי", "H3176", "HVhp1cs"]], "header": ""}
{"chapter": 130, "verse": 6, "english": "My soul <i>waiteth</i> for the Lord more than they that watch for the morning: <i>I say, more than</i> they that watch for the morning.", "hebrew": [["נפש/י", "H5315", "HNcbsc/Sp1cs"], ["ל/אדנ/י", "Hl/H136", "HR/Ncmpc/Sp1cs"], ["מ/שמרים", "Hm/H8104", "HR/Vqrmpa"], ["ל/בקר", "Hl/H1242", "HRd/Ncmsa"], ["שמרים", "H8104", "HVqrmpa"], ["ל/בקר", "Hl/H1242", "HRd/Ncmsa"]], "header": ""}
